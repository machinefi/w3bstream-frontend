{
  "name": "w3bstream-studio",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Starter pack using Nextjs, React, Typescript, tRPC, Mantine and Cypress",
  "keywords": [
    "next",
    "react",
    "typescript"
  ],
  "scripts": {
    "dev": "concurrently \"node node-docker-server\" \"next dev\"",
    "build": "next build",
    "build:standalone": "STANDALONE=true next build",
    "analyze": "ANALYZE=true next build",
    "export": "next build && next export",
    "start": "next start",
    "test": "concurrently \"yarn start \"",
    "test:ci": "cypress run --headless",
    "postinstall": "npm run gen",
    "gen": "prisma generate; prisma generate --schema ./prisma/monitor.prisma",
    "db:pull": "npx prisma db pull"
  },
